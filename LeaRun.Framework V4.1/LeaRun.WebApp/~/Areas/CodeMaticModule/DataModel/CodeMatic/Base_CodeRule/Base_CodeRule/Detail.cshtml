@{
    ViewBag.Title = "编码规则主表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/Base_CodeRule/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>编码规则主键：</th>
            <td class='formValue'>
                <label id="CodeRuleId" />
            </td>
            <th class='formTitle'>编码名称：</th>
            <td class='formValue'>
                <label id="FullName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>编码代号：</th>
            <td class='formValue'>
                <label id="Code" />
            </td>
            <th class='formTitle'>对应模块：</th>
            <td class='formValue'>
                <label id="ModuleId" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>有效：</th>
            <td class='formValue'>
                <label id="Enabled" />
            </td>
            <th class='formTitle'>排序码：</th>
            <td class='formValue'>
                <label id="SortCode" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>删除标记：</th>
            <td class='formValue'>
                <label id="DeleteMark" />
            </td>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreateDate" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建用户主键：</th>
            <td class='formValue'>
                <label id="CreateUserId" />
            </td>
            <th class='formTitle'>创建用户：</th>
            <td class='formValue'>
                <label id="CreateUserName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改时间：</th>
            <td class='formValue'>
                <label id="ModifyDate" />
            </td>
            <th class='formTitle'>修改用户主键：</th>
            <td class='formValue'>
                <label id="ModifyUserId" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>修改用户：</th>
            <td class='formValue'>
                <label id="ModifyUserName" />
            </td>
        </tr>
    </table>
</form>
