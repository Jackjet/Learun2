@{
    ViewBag.Title = "系统日志表》明细页面";
    Layout = "~/Views/Shared/_LayoutForm.cshtml";
}
<script type="text/javascript">
    $(function () {
        InitControl();
    })
    //得到一个对象实体
    function InitControl() {
        if (!!GetQuery('KeyValue')) {
            AjaxJson("/CommonModule/Base_SysLog/SetForm", { KeyValue: GetQuery('KeyValue') }, function (data) {
                SetWebControls(data);
            });
        }
    }
</script>
<form id="form1" style="margin: 1px">
    <table class="form">
        <tr>
            <th class='formTitle'>日志主键：</th>
            <td class='formValue'>
                <label id="SysLogId" />
            </td>
            <th class='formTitle'>对象主键：</th>
            <td class='formValue'>
                <label id="ObjectId" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>日志类型：</th>
            <td class='formValue'>
                <label id="LogType" />
            </td>
            <th class='formTitle'>操作IP：</th>
            <td class='formValue'>
                <label id="IPAddress" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>IP地址所在地址：</th>
            <td class='formValue'>
                <label id="IPAddressName" />
            </td>
            <th class='formTitle'>公司主键：</th>
            <td class='formValue'>
                <label id="CompanyId" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>部门主键：</th>
            <td class='formValue'>
                <label id="DepartmentId" />
            </td>
            <th class='formTitle'>创建时间：</th>
            <td class='formValue'>
                <label id="CreateDate" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>创建用户主键：</th>
            <td class='formValue'>
                <label id="CreateUserId" />
            </td>
            <th class='formTitle'>创建用户：</th>
            <td class='formValue'>
                <label id="CreateUserName" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>模块主键：</th>
            <td class='formValue'>
                <label id="ModuleId" />
            </td>
            <th class='formTitle'>描述：</th>
            <td class='formValue'>
                <label id="Remark" />
            </td>
        </tr>
        <tr>
            <th class='formTitle'>状态：</th>
            <td class='formValue'>
                <label id="Status" />
            </td>
        </tr>
    </table>
</form>
